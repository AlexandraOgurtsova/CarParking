        <Label Content="Изменить" HorizontalAlignment="Left" Margin="10,62,0,0" VerticalAlignment="Top"/>
        <TextBox Foreground="Black" x:Name="change1" HorizontalAlignment="Left" Height="23" Margin="181,57,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120"/>
        <Label Content="на" HorizontalAlignment="Left" Margin="319,56,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="change2" HorizontalAlignment="Left" Height="23" Margin="361,56,0,0" TextWrapping="Wrap" Foreground="black" VerticalAlignment="Top" Width="120"/>
        <Button Background="#ff4040" x:Name="change" BorderBrush="{x:Null}" Content="Изменить" HorizontalAlignment="Left" Margin="191,115,0,0" VerticalAlignment="Top" Width="123" Click="change_Click"/>
        <Label Foreground="Black" x:Name="where" HorizontalAlignment="Left" Margin="101,62,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
