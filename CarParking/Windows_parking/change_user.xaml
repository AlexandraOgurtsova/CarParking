        xmlns:local="clr-namespace:курсовой.Windows_parking"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        mc:Ignorable="d"  ResizeMode="NoResize" WindowStyle="None" Foreground="White"  WindowStartupLocation="CenterScreen" BorderBrush="Black" BorderThickness="0.5"
        Title="change_user" Height="198.769" Width="502.944"  MouseLeftButtonDown="Window_MouseLeftButtonDown">
    <Grid Margin="0,40,-1.667,-1.667">
        <Grid Background="#ff4040" HorizontalAlignment="Left" Height="42" Margin="0,-40,0,0" VerticalAlignment="Top" Width="503">
            <Button x:Name="Minimize" HorizontalAlignment="Right" Width="84" Height="35" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="White" Click="Minimize_Click" Margin="0,1,86,5"  >
                <materialDesign:PackIcon Kind ="WindowMinimize" Height="25" Width="20" />
            </Button>
            <Button x:Name="Close" HorizontalAlignment="Right" Background="{x:Null}" BorderBrush="{x:Null}" Foreground="White" Click="Close_Click" Margin="0,4,10,5" Width="76">
